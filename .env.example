# Environment
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/govtech_pro_dev

# Authentication
JWT_SECRET=your-super-secret-jwt-key-min-32-chars
JWT_EXPIRES_IN=7d
REFRESH_TOKEN_SECRET=your-super-secret-refresh-token-key-min-32-chars
REFRESH_TOKEN_EXPIRES_IN=30d

# Gov.br Integration
GOVBR_CLIENT_ID=your-govbr-client-id
GOVBR_CLIENT_SECRET=your-govbr-client-secret
GOVBR_REDIRECT_URI=http://localhost:3000/auth/govbr/callback
GOVBR_ENVIRONMENT=sandbox

# ICP-Brasil (Optional)
ICP_BRASIL_CERT_PATH=/path/to/certificate.p12
ICP_BRASIL_CERT_PASSWORD=certificate-password
ICP_BRASIL_TIMESTAMP_URL=http://timestamp.iti.gov.br

# File Storage
STORAGE_TYPE=local
# For S3 storage:
# STORAGE_TYPE=s3
# STORAGE_BUCKET=your-s3-bucket
# STORAGE_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your-aws-access-key
# AWS_SECRET_ACCESS_KEY=your-aws-secret-key

# Email (Optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password
SMTP_FROM=noreply@govtech-pro.com.br

# WhatsApp Business (Optional)
WHATSAPP_PHONE_NUMBER_ID=your-phone-number-id
WHATSAPP_ACCESS_TOKEN=your-whatsapp-access-token
WHATSAPP_WEBHOOK_VERIFY_TOKEN=your-webhook-verify-token
WHATSAPP_BUSINESS_ACCOUNT_ID=your-business-account-id

# Redis (Optional - for caching)
REDIS_URL=redis://localhost:6379

# Monitoring (Optional)
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# LGPD Compliance
DPO_EMAIL=dpo@govtech-pro.com.br
PRIVACY_POLICY_URL=https://govtech-pro.com.br/privacy
DATA_RETENTION_DAYS=2555

# TCU/TCE Integration (Optional)
TCU_ENTITY_CODE=your-entity-code
TCU_ENTITY_NAME=Prefeitura Municipal
TCU_CONTACT_EMAIL=transparencia@prefeitura.gov.br

# Transparency Portal (Optional)
TRANSPARENCY_PORTAL_URL=https://transparencia.prefeitura.gov.br
TRANSPARENCY_API_KEY=your-transparency-api-key

# AI Services (Optional)
OPENAI_API_KEY=sk-your-openai-api-key
AZURE_COGNITIVE_SERVICES_KEY=your-azure-cognitive-key
AZURE_COGNITIVE_SERVICES_ENDPOINT=https://your-region.cognitiveservices.azure.com/